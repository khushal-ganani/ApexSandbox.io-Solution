public String registerStudent(String name, String phone, String email) {
    // code here
    apxio__Student__c record = new apxio__Student__c();
    record.Name = name;
    record.apxio__Phone__c = phone;
    record.apxio__Email__c = email;
    insert record;
    Integer newRecordId = record.Id;
    apxio__Student__c newRecord = [SELECT apxio__Registration_Number__c FROM apxio__Student__c WHERE Id IN :newRecordId LIMIT 1];
    return newRecord.apxio__Registration_Number__c;
}