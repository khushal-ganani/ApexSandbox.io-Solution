public Integer nextPrime(Integer num) {
    //code here
    Integer nextPrimeNumber;
    for (Integer i = num+1; i++) {
        nextPrimeNumber = i;
        Boolean isPrime = true;
        for (Integer i = 2; i<num; i++) {
            if (Math.mod(num, i) == 0) {
                isPrime = false;
                break;
            }
        }
        if (isPrime) {
            break;
        }
    }
    return nextPrimeNumber;
}
