public Integer nextPrime(Integer num) {
    //code here
    Integer nextPrimeNumber = num;
    while (true) {
        nextPrimeNumber++;
        Boolean isPrime = true;
        for (Integer i = 2; i<nextPrimeNumber; i++) {
            if (Math.mod(num, i) == 0) {
                isPrime = false;
                break;
            }
        }
        if (isPrime) {
            break;
        }
    }
    return nextPrimeNumber;
}
